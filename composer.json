{
  "name": "nicksagona/pop-spider",
  "description": "A simple web spider for SEO analysis.",
  "keywords": [
    "seo",
    "web spider",
    "spider"
  ],
  "homepage": "https://github.com/nicksagona/pop-spider",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Nick Sagona",
      "email": "dev@nolainteractive.com",
      "homepage": "http://www.nolainteractive.com/"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "popphp/popphp": "^3.7.2",
    "popphp/pop-console": "^3.2.1",
    "popphp/pop-http": "^4.1.7",
    "popphp/pop-view": "^3.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0"
  },
  "autoload": {
    "psr-4": {
      "PopSpider\\": "app/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PopSpider\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  }
}
